{
    "name": "bst-co/laravel-postcode-ja",
    "type": "project",
    "version": "0.2.4",
    "keywords": [
        "laravel",
        "postalcode"
    ],
    "authors": [
        {
            "name": "t-ueno",
            "email": "t-ueno@bstinc.co.jp"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "laravel/framework": "^9.10.0|^10.0|^11.0",
        "ext-mbstring": "*",
        "ext-zip": "*"
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "orchestra/testbench": "^7.49"
    },
    "autoload": {
        "psr-4": {
            "BstCo\\PostcodeJa\\": "src/",
            "BstCo\\PostcodeJa\\Database\\Factories\\": "database/factories",
            "BstCo\\PostcodeJa\\Database\\Seeders\\": "database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BstCo\\PostcodeJa\\Test\\Unit\\": "tests/Unit",
            "BstCo\\PostcodeJa\\Test\\Feature\\": "tests/Feature"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BstCo\\PostcodeJa\\Providers\\PostcodeJaServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
